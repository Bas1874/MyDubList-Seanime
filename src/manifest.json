{
    "id": "MyDubList",
    "name": "MyDubList",
    "version": "1.1.1",
    "manifestURI": "https://raw.githubusercontent.com/Bas1874/MyDubList-Seanime/refs/heads/main/src/manifest.json",
    "language": "typescript",
    "type": "plugin",
    "description": "Adds a customizable Dubbed badge to anime cards in Seanime, indicating if a dub exists in your selected language based on data from MyDubList.com",
    "author": "bas1874",
    "icon": "https://raw.githubusercontent.com/Bas1874/MyDubList-Seanime/refs/heads/main/src/icons/logo.png",
    "website": "",
    "lang": "en",
    "payloadURI": "https://raw.githubusercontent.com/Bas1874/MyDubList-Seanime/refs/heads/main/src/MyDubList.ts",
    "plugin": {
        "version": "1",
        "permissions": {
            "scopes": [],
            "allow": {
                "unsafeFlags": [
                    {
                        "flag": "dom-script-manipulation",
                        "reason": "Needed to inject the custom Dub Badge UI elements onto anime cards."
                    }
                ]
            }
        }
    },
    "isDevelopment": false
}
